	</xsd:complexType>
	<xsd:complexType name="CopyrightStructure" mixed="true">
		<xsd:sequence maxOccurs="unbounded">
			<xsd:element name="Paragraph" type="ParagraphStructure"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="EditedStructure" mixed="true">
		<xsd:sequence maxOccurs="unbounded">
