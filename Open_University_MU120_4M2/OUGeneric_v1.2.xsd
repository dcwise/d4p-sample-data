			<xsd:element name="olink" type="olinkStructure"/>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="URI">
		<xsd:annotation>
			<xsd:documentation>
      a Uniform Resource Identifier, see [RFC2396]
			</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:anyURI"/>
	</xsd:simpleType>
	<xsd:simpleType name="ContentType">
